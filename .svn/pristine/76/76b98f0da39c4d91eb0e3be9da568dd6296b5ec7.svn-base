package com.hanson.model;

import java.util.Date;

//@Entity
//@Optlog(alias="产品")
//@Table(name = "pdt_product")
public class Product {
	
	private Long id;
	private String name;// 商品名称
	private Long price;//金额
	private Long oldprice; //现在金额
	private Date createtime;// 录入时间
	private String isnew;// 是否新品。n：否，y：是
	private String sale;// 是否特价。n：否，y：是
	private String isrecommend; //0不推荐 1推荐
	private Integer status; //状态 0 新增 1商家 2下架
	private Integer sellcount;//售出数量
	private String unit; //单位
	private Long score;//送积分
	private String catalogid;
	private Long qyid;
	private Integer sort;//排序
	private String specnameids = "[]"; //JSON格式的规格id列表
	private Integer bdelete;    // 删除位，数据库保存时，0为未删除，1为已被删除 
	private String producttype;
	
	public String getProducttype() {
		return producttype;
	}
	public void setProducttype(String producttype) {
		this.producttype = producttype;
	}
	public Long getId() {
		return id;
	}
	public void setId(Long id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public Long getPrice() {
		return price;
	}
	public void setPrice(Long price) {
		this.price = price;
	}
	public Long getOldprice() {
		return oldprice;
	}
	public void setOldprice(Long oldprice) {
		this.oldprice = oldprice;
	}
	public Date getCreatetime() {
		return createtime;
	}
	public void setCreatetime(Date createtime) {
		this.createtime = createtime;
	}
	public String getIsnew() {
		return isnew;
	}
	public void setIsnew(String isnew) {
		this.isnew = isnew;
	}
	public String getSale() {
		return sale;
	}
	public void setSale(String sale) {
		this.sale = sale;
	}
	public String getIsrecommend() {
		return isrecommend;
	}
	public void setIsrecommend(String isrecommend) {
		this.isrecommend = isrecommend;
	}
	public Integer getStatus() {
		return status;
	}
	public void setStatus(Integer status) {
		this.status = status;
	}
	public Integer getSellcount() {
		return sellcount;
	}
	public void setSellcount(Integer sellcount) {
		this.sellcount = sellcount;
	}
	public String getUnit() {
		return unit;
	}
	public void setUnit(String unit) {
		this.unit = unit;
	}
	public Long getScore() {
		return score;
	}
	public void setScore(Long score) {
		this.score = score;
	}
	public String getCatalogid() {
		return catalogid;
	}
	public void setCatalogid(String catalogid) {
		this.catalogid = catalogid;
	}
	public Long getQyid() {
		return qyid;
	}
	public void setQyid(Long qyid) {
		this.qyid = qyid;
	}
	public Integer getSort() {
		return sort;
	}
	public void setSort(Integer sort) {
		this.sort = sort;
	}
	public String getSpecnameids() {
		return specnameids;
	}
	public void setSpecnameids(String specnameids) {
		this.specnameids = specnameids;
	}
	public Integer getBdelete() {
		return bdelete;
	}
	public void setBdelete(Integer bdelete) {
		this.bdelete = bdelete;
	}
	
}